  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#FF5C8D"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Static types",
      "scope": [
        "entity.name.type",
        "storage.type.cs"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#D19A66",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#FF5C8D"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "Variable names",
      "scope": [
        "entity.name.variable",
        "variable"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.type.class.ts",
        "entity.name.type.interface.ts"
      ],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "variable.other.constant.property.ts",
        "variable.other.constant.object.property.ts"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#98C379",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup headings",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#D19A66",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#C678DD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "Storage keywords",
      "scope": [
        "storage",
        "keyword",
        "keyword.operator",
        "keyword.operator.new",
        "keyword.operator.logical",
        "keyword.operator.expression",
        "keyword.operator.delete",
        "variable.language"
      ],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "HTML derivative",
      "scope": "text.html.derivative",
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted",
        "string.template",
        "constant",
        "string.quoted.double",
        "string.quoted.single",
        "string.quoted.double.ts",
        "constant.numeric.decimal",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "Markup link descriptions",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#FF5C8D"
      }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "Provided functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "name": "Provieded variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": [
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#D19A66",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Tag brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator"
      ],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "name": "Markup list punctuation",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#E06C75"
      }
    }
  ]
}
